# Production Environment Variables
# SECURITY: Change all default values before deploying!

# Server Configuration
PORT=5000
NODE_ENV=production

# Database Configuration
# IMPORTANT: Change these credentials!
DB_HOST=postgres
DB_PORT=5432
DB_NAME=netpoint_db
DB_USER=netpoint_user
DB_PASSWORD=CHANGE_THIS_PASSWORD_IN_PRODUCTION
DB_SSL=false

# JWT Configuration
# Generate with: openssl rand -base64 32
JWT_SECRET=CHANGE_THIS_SECRET_KEY_IN_PRODUCTION

# CORS Configuration
# Set to your frontend domain
CORS_ORIGIN=https://yourdomain.com
